  # -------------------------
  RewriteCond %{REQUEST_URI} !^/articles/ [NC]
  RewriteCond %{REQUEST_URI} !^/articles$ [NC]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^(.*)$ public/index.php/$1 [L,QSA]

