  # WORDPRESS SECTION - PRIORITAS KEDUA
  # Semua URL yang mengandung /articles/ langsung ke WordPress
  # -------------------------
  RewriteRule ^articles/ - [L]
  RewriteRule ^articles$ - [L]

  # -------------------------
  # STATIC ASSETS
  # -------------------------
  RewriteRule ^(assets|css|js|images|uploads)(/.*)?$ public/$0 [L]

  # -------------------------
